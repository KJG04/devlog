{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "#types": ["src/types/index.d.ts"],
      "#types/*": ["src/types/*"],
      "#containers/*": ["src/containers/*"],
      "#utils/*": ["src/utils/*"],
      "#styles/*": ["src/styles/*"],
      "#components/*": ["src/components/*"],
      "#hooks/*": ["src/hooks/*"],
      "#atoms/*": ["src/atoms/*"],
      "#context/*": ["src/context/*"],
    },
    "typeRoots": ["src/types", "./node_modules/@types"],
    "plugins": [
      {
        "name": "next",
      },
    ],
  },
  "include": [
    "src/types/svg.d.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
  ],
  "exclude": ["node_modules"],
}
